@using MusStore.Models
@model  MusStore.Models.CategoryCompany

<div class="span3" style="margin-left: 0px" id='cssmenu'>
    <ul>
            @foreach (var item in Model.CompanyCategory)
            {
                <li class='has-sub'>
                    <a href='#'><span>@item.Key</span></a>
                    <ul>
                        @foreach (var company in @item.Value)
                        {
                            <li>
                                <a href="/Home/Index/@company.TopicId">@company.CompanyName</a>
                            </li>}
                    </ul>
                </li>
            }


       


    </ul>
</div>